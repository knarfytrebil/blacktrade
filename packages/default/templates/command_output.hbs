<paragraph 
    styles='{"fg": "cyan", "bg": "reset"}'
    wrap='{"trim": true}'
    alignment='{"position": "Left"}'>
    {{#height_buffer props.store.console_output_lines as |lines| ~}}
        {{#each lines as |line| ~}}
            <line>
                <span>{{line}}</span>
            </line>
        {{/each}}
    {{/height_buffer}}
</paragraph>